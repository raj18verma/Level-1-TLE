// Question Link:- https://codeforces.com/group/MWSDmqGsZm/contest/219432/problem/C

#include<bits/stdc++.h>
using namespace std;

int main(){

int n,even=0,odd=0,positive=0,negative=0;
cin>>n;
int arr[n];
for(int i=0; i<n; i++){
    cin>>arr[i];
}
for(int i=0; i<n; i++){
        if(arr[i]%2 == 0)
            even++;
        if(arr[i]%2!=0)
            odd++;
        if(arr[i]>0)
            positive++;
        if(arr[i]<0)
            negative++;
}
cout<<"Even: "<<even<<endl;
cout<<"Odd: "<<odd<<endl;
cout<<"Positive: "<<positive<<endl;
cout<<"Negative: "<<negative<<endl;

    return 0;
}

