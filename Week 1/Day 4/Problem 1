// Question Link:- https://codeforces.com/problemset/problem/1/A

#include<bits/stdc++.h>
using namespace std;

int main(){
       
       long long int n,m,a;
       cin>>n>>m>>a;
       
       n=n/a + (n%a!=0);
       m=m/a + (m%a!=0);
       
       cout<<n*m;
       
       return 0;
}


