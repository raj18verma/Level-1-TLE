// Question Link:- https://codeforces.com/problemset/problem/1030/A


#include<bits/stdc++.h>
using namespace std;

int main(){
       
       int n;
       cin>>n;
       int count=0;
       
       for(int i=0; i<n; i++){
              int num;
              cin>>num;
              if (num==1)
              count++;
       }
       if(count==0){
              cout<<"EASY";
       }else{
              cout<<"HARD";
       }
       
       return 0;
}
